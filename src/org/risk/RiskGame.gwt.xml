<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='risk_game'>
  <!-- Inherit the core Web Toolkit stuff. -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.googlecode.mgwt.MGWT"/>
  <inherits name='com.google.gwt.json.JSON'/>
  <inherits name='com.google.common.collect.Collect'/>
  <inherits name='org.vectomatic.libgwtsvg'/>
  <inherits name="com.google.gwt.i18n.I18N"/>
  
  <!-- Spanish language, independent of country -->
  <extend-property name="locale" values="es"/>
  
  <!-- <set-property name="user.agent" value="safari" />
  <set-property name="mgwt.os" value="desktop" />  -->

  <entry-point class='org.risk.graphics.RiskEntryPoint'/>
  <add-linker name='xsiframe' />

  <source path='logic'/>
  <source path='graphics'/>
</module>